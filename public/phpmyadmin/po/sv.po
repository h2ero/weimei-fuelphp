msgid "Commonly monitored"
msgstr "Vanligtvis övervakade"

#: server_status.php:1860
msgid "or type variable name:"
msgstr "eller skriv variabelnamn:"

#: server_status.php:1864
msgid "Display as differential value"
msgstr "Visa som skillnadsvärde"

#: server_status.php:1866
msgid "Apply a divisor"
msgstr "Tillämpa en dividend"

#: server_status.php:1873
msgid "Append unit to data values"
msgstr "Lägg till enhet till datavärdena"

#: server_status.php:1879
msgid "Add this series"
msgstr "Lägg till denna serie"

#: server_status.php:1881
msgid "Clear series"
msgstr "Rensa serie"

#: server_status.php:1884
msgid "Series in Chart:"
msgstr "Serie i diagram:"

#: server_status.php:1899
msgid "Log statistics"
msgstr "Logga statistik"

#: server_status.php:1900
msgid "Selected time range:"
msgstr "Välj tidsintervall:"

#: server_status.php:1905
msgid "Only retrieve SELECT,INSERT,UPDATE and DELETE Statements"
